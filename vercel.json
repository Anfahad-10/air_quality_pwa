{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*).(css|js|png|ico|json|svg)",
      "dest": "/public/$1.$2"
    },
    {
      "src": "/air-quality",
      "dest": "/server.js"
    },
    {
      "src": "/subscribe",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}